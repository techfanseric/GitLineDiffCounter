{
  "name": "git-line-diff-counter",
  "displayName": "Git Line Diff Counter",
  "description": "Show git working directory and staging area line change statistics in status bar",
  "version": "0.0.2",
  "publisher": "techfanseric",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other",
    "SCM"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "node esbuild.js",
    "watch": "node esbuild.js --watch",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "16.x",
    "esbuild": "^0.17.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "simple-git": "^3.16.0"
  }
}